{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "required": [
            "title",
            "link",
            "publicLink"
        ],
        "properties": {
            "title": {
                "type": "string"
            },
            "link": {
                "type": "string"
            },
            "publicLink": {
                "type": "string"
            },
            "path": {
                "type": "string"
            },
            "files": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "shouldSeed": {
                "type": "boolean"
            }
        }
    }
}